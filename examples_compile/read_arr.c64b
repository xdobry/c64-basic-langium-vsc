PRINT "START"
DIM A(5)
FOR X%=0 TO 5
   PRINT "READ ";X%
   READ A(X%)
NEXT X%
IF A(0)<> 1 THEN PRINT "ERROR1"
IF A(5)<> 6 THEN PRINT "ERROR2"
DATA 1,2,3,4,5,6
DIM A$(2)
FOR X%=0 TO 2
   PRINT "READ2 ";X%
   READ A$(X%)
NEXT X%
IF A$(0)<> "A" THEN PRINT "ERROR3"
IF A$(2)<> "C" THEN PRINT "ERROR4"
DATA "A","B","C"
PRINT "END"
